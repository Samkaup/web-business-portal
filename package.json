    "eslint-config-next": "^14.1.0",
    "form-data": "^4.0.0",
    "label": "^0.2.2",
    "lucide-react": "^0.294.0",
    "next": "^14.1.0",
    "next-seo": "^6.0.0",
    "next-sitemap": "^4.0.9",
    "next-themes": "^0.2.1",
    "popover": "^2.4.1",
    "react": "^18.2.0",
    "react-aria": "^3.26.0",
    "react-day-picker": "^8.9.1",
    "react-dom": "^18.2.0",
